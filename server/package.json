{
      "name": "css.app",
      "version": "1.0.0",
      "description": "",
      "main": "index.js",
      "scripts": {
            "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
            "dev:up": "docker-compose -p dstyle -f docker-compose.dev.yml up -d",
            "dev:build": "docker-compose -p dstyle -f docker-compose.dev.yml up -d --build",
            "dev:down": "docker-compose -p dstyle -f docker-compose.dev.yml down",
            "migrate:dev": "npx prisma migrate dev --schema ./src/lib/prisma/schema.prisma",
            "migrate:prod": "npx prisma migrate deploy --schema ./src/lib/prisma/schema.prisma",
            "generate": "npx prisma generate --schema ./src/lib/prisma/schema.prisma",
            "studio": "npx prisma studio --schema ./src/lib/prisma/schema.prisma",
            "env": "npx gen-env-types .env -o src/lib/env.types.ts -e ."
      },
      "repository": {
            "type": "git",
            "url": "git+https://github.com/audn/css.app.git"
      },
      "author": "",
      "license": "ISC",
      "bugs": {
            "url": "https://github.com/audn/css.app/issues"
      },
      "homepage": "https://github.com/audn/css.app#readme",
      "dependencies": {
            "@prisma/client": "^4.1.1",
            "chalk": "^4.1.1",
            "config": "^3.3.7",
            "connect-pg-simple": "^7.0.0",
            "cors": "^2.8.5",
            "dotenv": "^16.0.1",
            "dotenv-cli": "^6.0.0",
            "express": "^4.18.1",
            "express-rate-limit": "^6.5.1",
            "jsonwebtoken": "^8.5.1",
            "lodash": "^4.17.21",
            "nodemon": "^2.0.19",
            "oauth": "^0.10.0",
            "prisma": "^4.1.1",
            "request": "^2.88.2",
            "ts-node": "^10.9.1",
            "typescript": "^4.7.4",
            "zod": "^3.17.10"
      },
      "devDependencies": {
            "@types/body-parser": "^1.19.2",
            "@types/jsonwebtoken": "^8.5.8",
            "@types/config": "^0.0.41",
            "@types/cors": "^2.8.12",
            "@types/dotenv": "^8.2.0",
            "@types/express": "^4.17.13",
            "@types/lodash": "^4.14.182",
            "@types/node": "^18.6.2",
            "@types/oauth": "^0.9.1",
            "ts-node-dev": "^2.0.0",
            "typescript": "^4.7.4"
      }
}
